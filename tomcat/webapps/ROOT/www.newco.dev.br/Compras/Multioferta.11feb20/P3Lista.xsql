<button label="Proveedores" alt="IMG-0710" status="IMG-0710" caption="IMG-0286">
      <name_function>MostrarPag</name_function> 
      <param>'http://www.newco.dev.br/Empresas/Proveedores.xsql?OPCION=P'</param>
    </button>
     
</form>
