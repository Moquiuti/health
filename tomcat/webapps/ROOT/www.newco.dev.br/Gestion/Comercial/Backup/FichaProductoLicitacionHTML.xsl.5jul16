					<input type="hidden" name="IDDOC_{LIC_OFE_ID}" id="IDDOC_{LIC_OFE_ID}" value="{DOCUMENTO/ID}"/>
					<input type="hidden" name="NOMBREDOC_{LIC_OFE_ID}" id="NOMBREDOC_{LIC_OFE_ID}" value="{DOCUMENTO/NOMBRE}"/>
