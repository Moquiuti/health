					'{@CEN_INT_SOAP_POSTXML}',
					'{@CEN_INT_SOAP_HEADER}',
					'{@CEN_INT_SOAP_ACTION}',
					'{@CEN_INT_SOAP_XMLNS}'
				);
			ELSE			
				v_IDCentro:='{@CEN_ID}';
			END IF;
			
			CENTROS_PCK.Mantenimiento (US_ID,v_IDCentro,'{@EMP_ID}');
		END IF;
	END;
]]>
</xsql:include-owa>


<button label="Aceptar" caption="IMG-0020">
	<name_function>document.forms['frmManten'].submit</name_function>
</button>
</form>
</Mantenimiento>
