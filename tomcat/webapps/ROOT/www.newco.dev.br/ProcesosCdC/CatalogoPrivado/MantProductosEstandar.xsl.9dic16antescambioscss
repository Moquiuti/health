	//array si hay 3 desplegables
	<xsl:choose>
	<xsl:when test="/Mantenimiento/PRODUCTOESTANDAR/TODASSUBFAMILIAS">
		<xsl:for-each select="/Mantenimiento/PRODUCTOESTANDAR/TODASSUBFAMILIAS/CATEGORIA/FAMILIA">
