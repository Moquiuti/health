/*template boton*/
.botonBox{
	height:25px;
	float:left;
	margin:10px 0px;
}
.boton{
	width:150px;
	height:20px;
	padding-top:5px;
	float:right; /* PS 4jul2017 left;*/
	text-align:center;
	border:1px solid #ccc;
        border-radius: 5px;
        background-color:#cccccc;  
        /* Safari 4+ y Chrome 1+ */  
        background-image:-webkit-gradient(linear, left top, left bottom, color-stop(0, #f7f7f7), color-stop(1, #cccccc));  
        /* Safari 5.1+ y Chrome 10+ */  
        background-image:-webkit-linear-gradient(#f7f7f7, #cccccc);  
        /* Firefox 3.6+ */  
        background-image:-moz-linear-gradient(top, #f7f7f7, #cccccc);  
          /* Opera 11.10+ */  
        background-image:-o-linear-gradient(top, #f7f7f7, #cccccc);  
        /* Internet Explorer 5.5+ */  
        filter:progid:DXImageTransform.Microsoft.gradient(startColorStr='#f7f7f7', EndColorStr='#cccccc');  
}
.boton:hover{
        background-color:#C3D2E9;  
        /* Safari 4+ y Chrome 1+ */  
        background-image:-webkit-gradient(linear, left top, left bottom, color-stop(0, #f7f7f7), color-stop(1, #C3D2E9));  
        /* Safari 5.1+ y Chrome 10+ */  
        background-image:-webkit-linear-gradient(#f7f7f7, #C3D2E9);  
        /* Firefox 3.6+ */  
        background-image:-moz-linear-gradient(top, #f7f7f7, #C3D2E9);  
          /* Opera 11.10+ */  
        background-image:-o-linear-gradient(top, #f7f7f7, #C3D2E9);  
        /* Internet Explorer 5.5+ */  
        filter:progid:DXImageTransform.Microsoft.gradient(startColorStr='#f7f7f7', EndColorStr='#C3D2E9');  
	transition: all 1s ease-in-out;
}
.boton a{
     font-weight:bold;
	color:#333333;
        font-size:13px;
	text-decoration:none;
}

.boton a.grey, .botonLargo a.grey, .botonLargoVerdadNara a.grey{
	color:#C4C4C4;
}
