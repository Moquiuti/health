<?xml-stylesheet type="text/xsl" href="licInformePorProveedorHTML.xsl"?>

<InformeProveedor xmlns:xsql="urn:oracle-xsql" connection="newco" lang="spanish">
<xsql:include-param name="LANG" value="{@PRMLANG}"/>
<xsql:include-param name="US_ID"/>
<xsql:include-param name="LIC_ID"/>
<xsql:include-param name="IDCENTROCOMPRAS"/>
<xsql:include-param name="LIC_PROD_ID"/>
<xsql:include-param name="STYLE"/>
 
<xsql:include-owa>
<![CDATA[
	DECLARE
		US_ID		NUMBER(6);
		v_Res		VARCHAR2(100);
	BEGIN
		US_ID:=SEGURIDAD_PCK.CompruebaCodigoDeSesion('{@SES_ID}','OfertasSel');

		IF US_ID>=0 and '{@LIC_ID}' IS NOT NULL THEN
		
			utilidades_pck.debug('XSQL InformePorProveedor Lic_ID:{@LIC_ID} IDProveedor:{@FIDPROVEEDOR}. INICIO.');
		
			LICITACIONESINFORMES_PCK.Vencedores_XML(
				US_ID,
				'{@LIC_ID}',
				'S',					--	Incluir datos ofertas
				'{@IDCENTROCOMPRAS}',
				NVL('{@FIDPROVEEDOR}','-1')
			);

		ELSE
			HTP.P('<Sorry/>');
		END IF;
	END;
]]>
</xsql:include-owa>
</InformeProveedor>
