	//	Muestra las opciones de un submenu
	function SubMenu(ID)
	{
		
		SubmenuActual=ID;

		jQuery("#MENU_INICIAL").hide();
		jQuery("#SUBMENU_"+ID).show();
		
	}

	//	Sale del submenu volviendo al menú principal
	function SalirSubmenu()
	{
		jQuery("#SUBMENU_"+SubmenuActual).hide();
